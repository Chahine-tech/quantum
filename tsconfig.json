{
  "compilerOptions": {
    "module": "ESNext", // Change "commonjs" à "ESNext"
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "target": "ES6", // Assurez-vous que cela correspond à ES6 ou une version ultérieure
    "moduleResolution": "node"
  },
  "include": ["src/**/*"]
}
